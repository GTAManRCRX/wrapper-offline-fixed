{
	"name": "wrapper-offline",
	"description": "GoAnimate server emulator",
	"version": "2.1.4",
	"homepage": "https://discord.gg/Kf7BzSw",
	"license": "MIT",
	"private": true,
	"scripts": {
		"postinstall": "node scripts/fixModules.js",
		"dev": "concurrently \"NODE_OPTIONS='--no-deprecation' node scripts/build.mjs --dev\" \"vite dev\"",
		"build": "vite build && node scripts/build.mjs",
		"package": "npx electron-packager dist 'Wrapper offline' --icon=dist/favicon.ico --overwrite"
	},
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"@ffmpeg-installer/ffmpeg": "1.1.0",
		"@ffprobe-installer/ffprobe": "2.1.1",
		"@octanuary/httpz": "2.0.9",
		"adm-zip": "0.5.12",
		"file-type": "16.5.4",
		"fluent-ffmpeg": "2.1.3",
		"formidable": "2.1.2",
		"jimp": "0.22.12",
		"mime-types": "2.1.35",
		"modern-normalize": "3.0.1",
		"serve-handler": "6.1.6",
		"vue": "3.5.13",
		"vue-router": "4.5.0",
		"xmldoc": "1.1.2"
	},
	"devDependencies": {
		"@types/fluent-ffmpeg": "2.1.24",
		"@types/formidable": "3.4.5",
		"@types/node": "20.11.13",
		"@types/xmldoc": "1.1.9",
		"@vitejs/plugin-vue": "5.2.1",
		"concurrently": "9.1.2",
		"electron": "11.5.0",
		"electron-packager": "15.5.1",
		"esbuild": "0.27.0",
		"vite": "6.3.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GTAManRCRX/wrapper-offline-modded.git"
	},
	"author": "GTAManRCRX",
	"bugs": {
		"url": "https://github.com/GTAManRCRX/wrapper-offline-modded/issues"
	}
